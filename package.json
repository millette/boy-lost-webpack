{
  "name": "boy-lost-webpack",
  "description": "Exploring webpack and lost.",
  "license": "MIT",
  "version": "3.0.0",
  "repository": "millette/boy-lost-webpack",
  "author": {
    "name": "Robin Millette",
    "email": "robin@millette.info",
    "url": "http://robin.millette.info"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "start": "standard && webpack-dev-server",
    "lint": "standard",
    "make": "standard && webpack && mkdir -p dist/js/polyfill/ ; cp crossdomain.xml logo.png favicon.ico index.html bundle.js dist/ && cp js/polyfill/* dist/js/polyfill/"
  },
  "standard": {
    "ignore": [
      "js/polyfill/"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "jade": "^1.11.0",
    "jade-html-loader": "0.0.3",
    "lost": "^6.7.2",
    "postcss-loader": "^0.8.2",
    "postcss-responsive-type": "^0.3.3",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
